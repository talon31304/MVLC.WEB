@model MVLC.WEB.ViewModels.DropDownDiv



<a class="btn btn-secondary dropdown-toggle" href="#" role="button" id="@(Model.HTML_ID)" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
    @Model.MenuText
</a>
<div id="@(Model.HTML_ID+"_Items")" class="dropdown-menu" aria-labelledby="dropdownMenuLink">
    @for (int i = 0; i < Model.elements.Count(); i++)
            {
        <a href="#" onclick="@Model.GetOnClick(Model.elements[i])">@Model.elements[i].DropDownText</a><br />
    }
</div>

